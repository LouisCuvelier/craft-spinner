{
    "name": "louiscuvelier/spinner",
    "description": "Spinner lets you do fast, easy and bulk content spinning.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "spinner"
    ],
    "support": {
        "docs": "https://github.com/LouisCuvelier/spinner/blob/master/README.md",
        "issues": "https://github.com/LouisCuvelier/spinner/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Louis Cuvelier",
            "homepage": "https://www.louiscuvelier.com"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "louiscuvelier\\spinner\\": "src/"
        }
    },
    "extra": {
        "name": "Spinner",
        "handle": "spinner",
        "developer": "Louis Cuvelier",
        "developerUrl": "https://www.louiscuvelier.com",
        "documentationUrl": "https://github.com/LouisCuvelier/spinner/blob/master/README.md",
        "class": "louiscuvelier\\spinner\\Spinner"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        },
        "platform": {
            "php": "8.0.2"
        }
    }
}
